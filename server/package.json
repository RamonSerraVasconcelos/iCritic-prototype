{
    "name": "icritic",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "dev": "tsnd --inspect --exit-child --quiet -r tsconfig-paths/register ./src/index.ts",
        "build": "rm -rf dist && tsc",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
        "lint": "npx eslint ./",
        "migrate:start": "node ./node_modules/.bin/prisma migrate deploy && yarn start",
        "test": "jest --runInBand",
        "coverage": "jest --runInBand --coverage --coverageReporters='text-summary'",
        "generate": "node ./node_modules/.bin/prisma generate --schema='./prisma/schema.prisma'"
    },
    "author": "paulozaneripe <paulozaneripe@hotmail.com>, ramonvasconcelos <rserravasconcelos@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^4.6.1",
        "@types/multer": "^1.4.7",
        "bcrypt": "^5.0.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "latest",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.7.8",
        "path": "^0.12.7",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.14.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.21",
        "@types/nodemailer": "^6.4.6",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "eslint": "^8.10.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "prettier": "^2.6.2",
        "prisma": "^4.6.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.6.2"
    }
}
